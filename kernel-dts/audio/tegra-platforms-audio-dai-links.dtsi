		};
		nvidia,dai-link-9 {
			link-name = "dmic3-capture";
			cpu-dai = <&tegra_dmic3>;
			codec-dai = <&tegra_dmic3>;
			cpu-dai-name = "DMIC3";
			codec-dai-name = "DUMMY_SOURCE";
			format = "i2s";
			bit-format = "s16_le";
			srate = <48000>;
			ignore_suspend;
			num-channel = <2>;
			status = "okay";
		};
		nvidia,dai-link-10 {
			link-name = "dmic4-capture";
			cpu-dai = <&tegra_dmic4>;
			codec-dai = <&tegra_dmic4>;
			cpu-dai-name = "DMIC4";
			codec-dai-name = "DUMMY_SOURCE";
			format = "i2s";
			bit-format = "s16_le";
			srate = <48000>;
			ignore_suspend;
			num-channel = <2>;
			status = "okay";
		};
		nvidia,dai-link-11 {
			link-name = "dspk1-playback";
			cpu-dai = <&tegra_dspk1>;
			codec-dai = <&tegra_dspk1>;
			cpu-dai-name = "DSPK1";
			codec-dai-name = "DUMMY_SINK";
			format = "i2s";
			bit-format = "s16_le";
			srate = <48000>;
			num-channel = <2>;
			ignore_suspend;
			status = "okay";
		};
		nvidia,dai-link-12 {
			link-name = "dspk2-playback";
			cpu-dai = <&tegra_dspk2>;
			codec-dai = <&tegra_dspk2>;
			cpu-dai-name = "DSPK2";
			codec-dai-name = "DUMMY_SINK";
