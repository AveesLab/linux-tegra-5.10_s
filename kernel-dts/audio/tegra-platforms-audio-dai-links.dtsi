			codec-dai = <&tegra_dmic1>;
			cpu-dai-name = "DMIC1";
			codec-dai-name = "DUMMY_SOURCE";
			format = "i2s";
			bit-format = "s16_le";
			srate = <48000>;
			ignore_suspend;
			num-channel = <2>;
			status = "okay";
		};
		nvidia,dai-link-8 {
			link-name = "dmic2-capture";
			cpu-dai = <&tegra_dmic2>;
			codec-dai = <&tegra_dmic2>;
			cpu-dai-name = "DMIC2";
