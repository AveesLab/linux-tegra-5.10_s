			codec-dai-name = "DUMMY";
			format = "i2s";
			bit-format = "s16_le";
			srate = <8000>;
			num-channel = <1>;
			ignore_suspend;
			status = "okay";
		};
		nvidia,dai-link-7 {
			link-name = "dmic1-capture";
