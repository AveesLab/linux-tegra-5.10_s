#include "tegra-platforms-audio-simple-bus.dtsi"
#include "tegra-platforms-audio-enable.dtsi"
/ {
	aconnect@2a41000 {
		adsp_audio {
			compr-ops = <1>;
			num-plugin = <6>;
			plugin-info-1 {
				plugin-name = "mp3-dec1";
				firmware-name = "nvmp3dec.elf";
				widget-name = "MP3-DEC1";
			};
			plugin-info-2 {
				plugin-name = "spkprot";
				firmware-name = "nvspkprot.elf";
				widget-name = "SPKPROT-SW";
			};
			plugin-info-3 {
				plugin-name = "src";
				firmware-name = "nvsrc.elf";
				widget-name = "SRC";
			};
			plugin-info-4 {
				plugin-name = "aac-dec1";
				firmware-name = "nvaacdec.elf";
				widget-name = "AAC-DEC1";
			};
			plugin-info-5 {
				plugin-name = "aec";
				firmware-name = "nvoice.elf";
				widget-name = "AEC";
			};
			plugin-info-6 {
				plugin-name = "wire";
				firmware-name = "libnvwirefx.elf";
				widget-name = "WIRE";
			};
		};
	};

	tegra_sound: sound {
		status = "disabled";
		nvidia,num-codec-link = <12>;
		nvidia,num-clk = <8>;
		nvidia,clk-rates = < 270950400	/* PLLA_x11025_RATE */
				     11289600	/* AUD_MCLK_x11025_RATE */
				     45158400	/* PLLA_OUT0_x11025_RATE */
				     45158400	/* AHUB_x11025_RATE */
				     245760000  /* PLLA_x8000_RATE */
				     12288000	/* AUD_MCLK_x8000_RATE */
				     49152000	/* PLLA_OUT0_x8000_RATE */
				     49152000 >;/* AHUB_x8000_RATE */

		nvidia,xbar = <&tegra_axbar>;

		nvidia,dai-link-1 {
			link-name = "i2s1-link";
			cpu-dai = <&tegra_i2s1>;
			codec-dai = <&tegra_i2s1>;
			cpu-dai-name = "I2S1";
			codec-dai-name = "DUMMY";
			format = "i2s";
			bit-format = "s16_le";
			srate = <48000>;
			num-channel = <2>;
			ignore_suspend;
			status = "okay";
		};
		nvidia,dai-link-2 {
			link-name = "i2s2-link";
			cpu-dai = <&tegra_i2s2>;
			codec-dai = <&tegra_i2s2>;
			cpu-dai-name = "I2S2";
			codec-dai-name = "DUMMY";
			format = "i2s";
			bit-format = "s16_le";
			srate = <48000>;
			num-channel = <2>;
			ignore_suspend;
			status = "okay";
		};
		nvidia,dai-link-3 {
			link-name = "i2s3-link";
			cpu-dai = <&tegra_i2s3>;
			codec-dai = <&tegra_i2s3>;
			cpu-dai-name = "I2S3";
			codec-dai-name = "DUMMY";
			format = "i2s";
			bit-format = "s16_le";
			srate = <48000>;
			num-channel = <2>;
			ignore_suspend;
			status = "okay";
		};
		nvidia,dai-link-4 {
			link-name = "i2s4-link";
			cpu-dai = <&tegra_i2s4>;
			codec-dai = <&tegra_i2s4>;
			cpu-dai-name = "I2S4";
			codec-dai-name = "DUMMY";
			format = "i2s";
			bit-format = "s16_le";
			srate = <48000>;
			num-channel = <2>;
			ignore_suspend;
			status = "okay";
		};
		nvidia,dai-link-5 {
			link-name = "i2s5-link";
			cpu-dai = <&tegra_i2s5>;
			codec-dai = <&tegra_i2s5>;
			cpu-dai-name = "I2S5";
			codec-dai-name = "DUMMY";
			format = "i2s";
			bit-format = "s16_le";
			srate = <48000>;
			num-channel = <2>;
			ignore_suspend;
			status = "okay";
		};
		nvidia,dai-link-6 {
